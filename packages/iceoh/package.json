{
  "name": "iceoh",
  "version": "3.0.0",
  "license": "MIT",
  "type": "module",
  "source": "src/index.ts",
  "unpkg": "./dist/index.umd.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "main": "./dist/iceoh.umd.cjs",
  "module": "./dist/iceoh.js",
  "exports": {
    ".": {
      "import": "./dist/iceoh.js",
      "require": "./dist/iceoh.umd.cjs"
    }
  },
  "keywords": [
    "pixi",
    "pixi.js",
    "phaser",
    "isometric",
    "minimalist",
    "small",
    "game",
    "gamedev",
    "tilemap"
  ],
  "author": "Sam McCord <sam.mccord@protonmail.com>",
  "bugs": {
    "url": "https://github.com/sammccord/iceoh/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sammccord/iceoh.git"
  },
  "scripts": {
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "build": "NODE_ENV=production vite build && tsc --emitDeclarationOnly --declaration --project ./tsconfig.json",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "dependencies": {}
}
